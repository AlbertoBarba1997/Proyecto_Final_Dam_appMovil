<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectoappgimnasio">

    <uses-permission android:name = "android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name = "android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name = "android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconoapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconoapp"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LoginActivity" android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".HorarioActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".SingUpActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".InfoClaseActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".ReservasActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".InfoReservaActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".TablasActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".EjerciciosActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".InfoEjercicioActivity" android:screenOrientation="portrait"></activity>



    </application>

</manifest>